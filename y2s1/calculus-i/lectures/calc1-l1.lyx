\begin_layout Standard
\begin_inset Formula 
\begin{align*}
x & =cos\left(t\right)\\
t & =cos^{-1}\left(x\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
y & =sin\left(t\right)\\
 & =sin\left(cos^{-1}\left(x\right)\right)\\
\boldsymbol{y} & \boldsymbol{=tan\left(x\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Alternatively:
\begin_inset Formula 
\begin{align*}
x^{2} & =cos^{2}\left(t\right)\\
y^{2} & =sin^{2}\left(t\right)\\
cos^{2}\left(t\right)+sin^{2}\left(t\right) & =1\\
x^{2}+y^{2} & =1
\end{align*}

\end_inset


\end_layout

