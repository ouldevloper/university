\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $y=2x^{4}-5x^{3}+3x^{2}-2x+4$
\end_inset


\begin_inset Formula 
\begin{align*}
\dx & =8x^{3}-15x^{2}+6x-2\\
\frac{d^{2}y}{dx^{2}} & =24x^{2}-30x+6
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=e^{-x}\sin2x$
\end_inset


\begin_inset Formula 
\begin{align*}
y' & =\frac{d}{dx}\left(e^{-x}\right)\cdot\sin2x+e^{-x}\cdot\frac{d}{dx}\left(\sin2x\right)\\
 & =\frac{d}{dx}\left(e^{-x}\right)\cdot\sin2x+e^{-x}\cdot\frac{d}{dx}\left(\sin2x\right)\\
 & =-e^{-x}\cdot\sin2x+e^{-x}\cdot\cos2x\cdot2\\
 & =-e^{-x}\sin2x+2e^{-x}\cos2x\\
 & =2e^{-x}\cos2x-e^{-x}\sin2x\\
 & =e^{-x}\left(2\cos2x-\sin2x\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
y'' & =\frac{d}{dx}\left[e^{-x}\left(2\cos2x-\sin2x\right)\right]\\
 & =\frac{d}{dx}\left[e^{-x}\right]*\left(2\cos2x-\sin2x\right)+e^{-x}\cdot\frac{d}{dx}\left[\left(2\cos2x-\sin2x\right)\right]\\
 & =-e^{-x}\left(2\cos2x-\sin2x\right)+e^{-x}\left(2\left(\left(-\sin2x\right)\cdot2\right)-\left(\cos2x\right)\cdot2\right)\\
 & =-e^{-x}\left(2\cos2x-\sin2x\right)+e^{-x}\left(-4\sin2x-2\cos2x\right)\\
 & =-e^{-x}\left(2\cos2x-\sin2x\right)-2e^{-x}\left(2\sin2x+\cos2x\right)\\
 & =-e^{-x}\left(2\cos2x-\sin2x+2\left(2\sin2x+\cos2x\right)\right)\\
 & =-e^{-x}\left(2\cos2x-\sin2x+4\sin2x+2\cos2x\right)\\
 & =-e^{-x}\left(4\cos2x+3\sin2x\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{\ln x}{x}$
\end_inset


\begin_inset Formula 
\begin{align*}
y' & =\frac{x\frac{d}{dx}\left(\ln x\right)-\ln x\frac{d}{dx}x}{x^{2}}\\
 & =\frac{x\frac{1}{x}-\ln x\cdot1}{x^{2}}\\
y' & =\frac{1-\ln x}{x^{2}}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
y'' & =\frac{x^{2}\frac{d}{dx}\left(1-\ln x\right)-\left(1-\ln x\right)\frac{d}{dx}\left(x^{2}\right)}{\left(x^{2}\right)^{2}}\\
 & =\frac{x^{2}\left(-\frac{1}{x}\right)-\left(1-\ln x\right)2x}{x^{4}}\\
 & =\frac{-x-\left(2x-2x\ln x\right)}{x^{4}}\\
 & =\frac{-3x+2x\ln x}{x^{4}}\\
 & =\frac{x\left(2\ln x-3\right)}{x^{4}}\\
 & =\frac{\left(2\ln x-3\right)}{x^{3}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $x=t^{3},y=t^{2}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
First derivative
\begin_inset Formula 
\begin{align*}
\frac{dx}{dt} & =3t^{2}\\
\frac{dy}{dt} & =2t
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Use chain rule
\begin_inset Formula 
\begin{align*}
\frac{dy}{dx} & =\frac{dy}{dt}*\frac{dt}{dx}\\
 & =\frac{dy}{dt}*\frac{1}{\frac{dx}{dt}}\\
 & =\frac{2t}{3t^{2}}\\
\frac{dy}{dx} & =\frac{2}{3t}
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Second derivative
\begin_inset Formula 
\begin{align*}
\frac{d^{2}y}{dx^{2}} & =\frac{d}{dx}\left[\frac{2}{3t}\right]\\
 & =\frac{2}{3}\frac{d}{dx}\left[t^{-1}\right]\\
 & =\frac{2}{3}\cdot-t^{-2}\cdot\frac{dt}{dx}\\
 & =-\frac{2}{3}t^{-2}\cdot\left(\frac{1}{\frac{dx}{dt}}\right)\\
 & =-\frac{2}{3t^{2}}\cdot\left(\frac{1}{3t^{2}}\right)\\
 & =-\frac{2}{9t^{4}}
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $x^{2}+y^{2}-2x+2y=23$
\end_inset

at point 
\begin_inset Formula $x=-2,y=3$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left[x^{2}+y^{2}-2x+2y\right] & =\frac{d}{dx}\left[23\right]\\
2x+2y\cdot\frac{dy}{dx}-2+\frac{dy}{dx}\cdot2 & =0\\
2y\dx+2\dx & =-\left(2x-2\right)\\
\dx\left(2y+2\right) & =2-2x\\
\dx & =\frac{2-2x}{\left(2y+2\right)}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left[\frac{dy}{dx}\right] & =\frac{d}{dx}\left[\frac{2-2x}{\left(2y+2\right)}\right]\\
\frac{d^{2}y}{dx^{2}} & =\frac{\left(2y+2\right)\frac{d}{dx}\left(2-2x\right)-\left(2-2x\right)\frac{d}{dx}\left(2y+2\right)}{\left(2y+2\right)^{2}}\\
 & =\frac{\left(2y+2\right)\left(-2\right)-\left(2-2x\right)\left(2\dx\right)}{\left(2y+2\right)^{2}}\\
 & =\frac{-4y-4+\left(4x-4\right)\left(\dx\right)}{\left(2y+2\right)^{2}}\\
 & =\frac{-4y-4+\left(4x-4\right)\left(\frac{2-2x}{\left(2y+2\right)}\right)}{\left(2y+2\right)^{2}}\\
 & =\frac{-4y-4+\left(\frac{\left(2-2x\right)\left(4x-4\right)}{\left(2y+2\right)}\right)}{\left(2y+2\right)^{2}}\\
 & =\frac{-4y-4+\left(\frac{2\left(1-x\right)\left(4x-4\right)}{2\left(y+1\right)}\right)}{\left(2y+2\right)^{2}}\\
 & =\frac{-4y-4+\left(\frac{\left(1-x\right)\left(4x-4\right)}{\left(y+1\right)}\right)}{\left(2y+2\right)^{2}}
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Substitute in the point 
\begin_inset Formula $\left(-2,3\right)$
\end_inset

where 
\begin_inset Formula $x=-2$
\end_inset

, 
\begin_inset Formula $y=3$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{d^{2}y}{dx^{2}}|_{x=-2,y=3} & =\frac{-4\left(3\right)-4+\left(\frac{\left(1-\left(-2\right)\right)\left(4\left(-2\right)-4\right)}{\left(3+1\right)}\right)}{\left(2\left(3\right)+2\right)^{2}}\\
 & =\frac{-12-4+\left(\frac{3\left(-12\right)}{4}\right)}{\left(6+2\right)^{2}}\\
 & =\frac{-16+\left(\frac{-36}{4}\right)}{\left(8\right)^{2}}\\
 & =\frac{-16-9}{\left(8\right)^{2}}\\
 & =\frac{-25}{64}\\
 & =-\frac{25}{64}
\end{align*}

\end_inset


