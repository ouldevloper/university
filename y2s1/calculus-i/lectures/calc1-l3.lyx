\begin_layout Enumerate
\begin_inset Formula $\frac{d}{dx}\left(\tan x\right)=\sec^{2}\left(x\right)$
\end_inset

 (REPROOF WITH SIMPLER METHOD)
\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left(\tan x\right) & =\frac{d}{dx}\left(\frac{\sin\left(x\right)}{\cos\left(x\right)}\right)\\
 & =\frac{\cos\left(x\right)\frac{d}{dx}\left(\sin x\right)-\sin\left(x\right)\frac{d}{dx}\left(\cos\left(x\right)\right)}{\left(cosx\right)^{2}}\\
 & =\frac{\cos\left(x\right)\cdot\left(\cos\left(x\right)\right)-\sin\left(x\right)\cdot\left(-\sin\left(x\right)\right)}{cos^{2}x}\\
 & =\frac{\cos^{2}x+\sin^{2}\left(x\right)}{\cos^{2}x};\text{note: \ensuremath{sin^{2}\left(x\right)+cos^{2}\left(x\right)=1}}\\
 & =\frac{1}{\cos^{2}x}\\
 & =\sec^{2}x
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\frac{d}{dx}\left(\cot x\right)=-csc^{2}\left(x\right)$
\end_inset

 (REPROOF WITH SIMPLER METHOD)
\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left(\cot x\right) & =\frac{d}{dx}\left(\tan^{-1}\left(x\right)\right)\\
 & =\frac{d}{dx}\left(\frac{cos\left(x\right)}{sin\left(x\right)}\right)\\
 & =\frac{d}{dx}\left(\frac{cos\left(x\right)}{sin\left(x\right)}\right)\\
 & =\frac{\sin\left(x\right)\frac{d}{dx}\left(cos\left(x\right)\right)-\cos\left(x\right)\frac{d}{dx}\left(\sin\left(x\right)\right)}{\left(sin\left(x\right)\right)^{2}}\\
 & =\frac{\sin\left(x\right)\left(-\sin\left(x\right)\right)-\cos\left(x\right)\left(\cos\left(x\right)\right)}{\left(sin\left(x\right)\right)^{2}}\\
 & =\frac{-\left(\sin^{2}\left(x\right)\right)-cos^{2}\left(x\right)}{\left(sin\left(x\right)\right)^{2}}\\
 & =\frac{-\left(sin^{2}\left(x\right)+cos^{2}\left(x\right)\right)}{sin^{2}\left(x\right)}\\
 & =-\frac{1}{sin^{2}\left(x\right)}\\
 & =-\csc^{2}\left(x\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\frac{d}{dx}\left(\sec x\right)=\sec x\tan x$
\end_inset

 (REPROOF WITH SIMPLER METHOD)
\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left(\sec x\right) & =\frac{d}{dx}\left(\frac{1}{cos\left(x\right)}\right)\\
 & =\frac{d}{dx}\left(\frac{1}{cos\left(x\right)}\right)\\
 & =\frac{d}{dx}\left(\frac{\tan\left(x\right)}{\sin\left(x\right)}\right)\\
 & =\frac{\sin\left(x\right)\frac{d}{dx}\left(\tan\left(x\right)\right)-\tan\left(x\right)\frac{d}{dx}\left(\sin\left(x\right)\right)}{sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(\sec^{2}\left(x\right)\right)-\tan\left(x\right)\left(\cos\left(x\right)\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(\sec^{2}\left(x\right)\right)-\frac{\sin\left(x\right)}{\cos\left(x\right)}\left(\cos\left(x\right)\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(\sec^{2}\left(x\right)\right)-\sin\left(x\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(\sec^{2}\left(x\right)-1\right)}{\sin^{2}\left(x\right)}\\
 & =\left(\sec^{2}\left(x\right)-1\right)\cdot\left(\frac{1}{sin\left(x\right)}\right)\\
 & =\left(\tan^{2}\left(x\right)\right)\cdot\left(\frac{1}{\sin\left(x\right)}\right)\\
 & =\tan\left(x\right)\cdot\frac{\cancel{\sin\left(x\right)}}{\cos\left(x\right)}\cdot\frac{1}{\cancel{\sin\left(x\right)}}\\
 & =\tan\left(x\right)\cdot\frac{1}{cos\left(x\right)}\\
 & =\tan\left(x\right)\sec\left(x\right)\\
\boldsymbol{\frac{d}{dx}\left(\sec x\right)} & \boldsymbol{=\sec\left(x\right)\tan\left(x\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\frac{d}{dx}\left(\csc\left(x\right)\right)=-\csc x\cot x$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left(\csc\left(x\right)\right) & =\frac{d}{dx}\left(\frac{1}{\sin\left(x\right)}\right)\\
 & =\frac{\sin\left(x\right)\frac{d}{dx}\left(1\right)-\frac{d}{dx}\left(\sin\left(x\right)\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{\sin\left(x\right)\left(0\right)-\cos\left(x\right)}{\sin^{2}\left(x\right)}\\
 & =\frac{-\cos\left(x\right)}{\sin^{2}\left(x\right)}\\
 & =-\csc\left(x\right)\cot\left(x\right)
\end{align*}

\end_inset


\end_layout

