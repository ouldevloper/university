\begin_inset Formula $B$
\end_inset

, when 
\begin_inset Formula $x=-1$
\end_inset


\begin_inset Formula 
\begin{align*}
7 & =A\left(-1+1\right)+B\left(-1-6\right)\\
 & =-7B\\
B & =-1
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Obtain final equation
\begin_inset Formula 
\begin{align*}
\frac{7}{x^{2}-5x-6} & =\frac{1}{\left(x-6\right)}-\frac{1}{\left(x+1\right)}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\ix{_{7}^{10}\frac{7}{x^{2}-5x-6}} & =\ix{_{7}^{10}\frac{A}{\left(x-6\right)}+\frac{B}{\left(x+1\right)}}\\
 & =\ix{_{7}^{10}\frac{1}{\left(x-6\right)}-\frac{1}{\left(x+1\right)}}\\
 & =\left[\ln\left(x-6\right)-\ln\left(x+1\right)\right]_{7}^{10}\\
 & =\left(\ln\left(10-6\right)-\ln\left(10+1\right)\right)-\left(\ln\left(7-6\right)-\ln\left(7+1\right)\right)\\
 & =\left(\ln4-\ln11\right)-\left(\ln1-\ln8\right)\\
 & =\ln4-\ln11-\ln1+\ln8\\
 & =\ln\frac{4*8}{11*1}\\
 & =\ln\frac{32}{11}
\end{align*}

\end_inset


\end_layout

