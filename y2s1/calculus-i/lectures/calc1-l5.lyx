\begin_layout Enumerate
\begin_inset Formula $\ix{\frac{3x^{2}-1}{\left(x^{3}-x+4\right)^{4}}}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=\left(x^{3}-x+4\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =3x^{2}-1\\
dx & =\frac{du}{3x^{2}-1}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute into question
\begin_inset Formula 
\begin{align*}
\ix{\frac{3x^{2}-1}{\left(x^{3}-x+4\right)^{4}}} & =\int\left(3x^{2}-1\right)u^{-4}\frac{du}{\left(3x^{2}-1\right)}\\
 & =\int u^{-4}du\\
 & =\frac{u^{-3}}{-3}+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute back in 
\begin_inset Formula $u=\left(x^{3}-x+4\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\ix{\frac{3x^{2}-1}{\left(x^{3}-x+4\right)^{4}}} & =-\frac{1}{3\left(x^{3}-x+4\right)}+c\\
 & =\frac{1}{-3x^{2}+x-4}+c
\end{align*}

\end_inset


\end_layout

\end_deeper
