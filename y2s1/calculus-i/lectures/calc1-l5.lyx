\begin_layout Enumerate
\begin_inset Formula $\int\left(2+3x\right)^{15}dx$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=2+3x$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =3\\
dx & =\frac{du}{3}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find the derivative with respect to 
\begin_inset Formula $u$
\end_inset


\begin_inset Formula 
\begin{align*}
\int u^{15}\frac{du}{3} & =\frac{1}{3}\int u^{15}du\\
 & =\frac{1}{3}\left(\frac{u^{16}}{16}\right)+c\\
 & =\frac{u^{16}}{48}+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute 
\begin_inset Formula $2+3x$
\end_inset

 back in to replace 
\begin_inset Formula $u$
\end_inset

 
\begin_inset Formula 
\[
\int\left(2+3x\right)^{15}dx=\frac{\left(2+3x\right)^{16}}{48}+c
\]

\end_inset


\end_layout

\end_deeper
