\end_inset

, when 
\begin_inset Formula $x=6$
\end_inset


\begin_inset Formula 
\begin{align*}
7 & =A\left(6+1\right)+B\left(6-6\right)\\
 & =7A\\
A & =1
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
