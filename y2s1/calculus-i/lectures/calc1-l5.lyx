\begin_layout Enumerate
Split into two terms
\begin_inset Formula 
\[
\int\sin^{n}x\,dx=\ix{\sin^{n-1}x\cdot\sin x}
\]

\end_inset


\end_layout

\begin_layout Enumerate
Find 
\begin_inset Formula $u$
\end_inset

 and 
\begin_inset Formula $dv$
\end_inset

, and use them to find 
\begin_inset Formula $du$
\end_inset

 and 
\begin_inset Formula $v$
\end_inset


\begin_inset Formula 
\begin{align*}
u & =\sin^{n-1}x\\
\frac{du}{dx} & =\left(n-1\right)\sin^{n-2}x\cos x\\
du & =\left(n-1\right)\sin^{n-2}x\cos x\,dx
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
dv & =\sin x\,dx\\
v & =-\cos x
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Utilize the formula 
\series bold
(Note to self: Look this up later on)
\series default

\begin_inset Formula 
\begin{align*}
\int\sin^{n}x\,dx & =\ix{\sin^{n-1}x\cdot\sin x}\\
 & =uv-\int v\,du\\
 & =\left(\sin^{n-1}x\right)\left(-\cos x\right)-\int\left(-\cos x\right)\left(n-1\right)\sin^{n-2}x\cos x\,dx\\
 & =\left(\sin^{n-1}x\right)\left(-\cos x\right)-\int\left(-\cos x\right)\left(n-1\right)\sin^{n-2}x\cos x\,dx\\
 & =-\cos x\sin^{n-1}x+\left(n-1\right)\int\sin^{n-2}x\cos x\cos x\,dx\\
 & =-\cos x\sin^{n-1}x+\left(n-1\right)\int\sin^{n-2}x\cos^{2}x\,dx\\
 & =-\cos x\sin^{n-1}x+\left(n-1\right)\int\sin^{n-2}x\left(1-\sin^{2}x\right)\,dx\\
 & =-\cos x\sin^{n-1}x+\left(n-1\right)\int\sin^{n-2}x-\sin^{n}x\,dx\\
 & =-\cos x\sin^{n-1}x+\left(n-1\right)\int\sin^{n-2}x-\left(n-1\right)\int\sin^{n}x\,dx\\
 & =-\cos x\sin^{n-1}x+\left(n-1\right)\int\sin^{n-2}x-\left(n-1\right)\int\sin^{n}x\,dx\\
 & =-\cos x\sin^{n-1}x-\left(n-1\right)\int\sin^{n}x\,dx+\left(n-1\right)\int\sin^{n-2}x\,dx\\
\left[1+\left(n-1\right)\right]\int\sin^{n}x\,dx & =-\cos x\sin^{n-1}x+\left(n-1\right)\int\sin^{n-2}x\,dx\\
n\int\sin^{n}xdx & =-\frac{1}{n}\cos x\sin^{n-1}x+\frac{n-1}{n}\int\sin x\,dx\\
 & =-\frac{1}{3}\cos x\sin^{2}x+\frac{2}{3}\left(-\cos x\right)+c\\
 & =-\frac{1}{3}\cos x\sin^{2}x-\frac{2}{3}\cos x+c
\end{align*}

\end_inset


