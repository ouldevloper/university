\begin_inset Formula $\ix{x\ln x}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $u=\ln x$
\end_inset

, 
\begin_inset Formula $dv=xdx$
\end_inset


\begin_inset Formula 
\[
du=\frac{1}{x}dx
\]

\end_inset


\begin_inset Formula 
\begin{align*}
\int dv & =\int xdx\\
v & =\frac{x^{2}}{2}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\ix{x\ln x} & =uv-\int vdu\\
 & =\ln x\left(\frac{x^{2}}{2}\right)-\int\left(\frac{x^{2}}{2}\right)\left(\frac{1}{x}\right)dx\\
 & =\frac{x^{2}}{2}\ln x-\ix{\frac{x}{2}}\\
 & =\frac{x^{2}}{2}\ln x-\frac{1}{2}\ix x\\
 & =\frac{x^{2}}{2}\ln x-\frac{1}{2}\left(\frac{x^{2}}{2}+c\right)\\
 & =\frac{x^{2}}{2}\ln x-\frac{x^{2}}{4}+c
\end{align*}

\end_inset


\end_layout

\end_deeper
