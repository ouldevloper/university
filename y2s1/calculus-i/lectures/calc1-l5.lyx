\begin_layout Enumerate
\begin_inset Formula $\ix{x\left(2-3x\right)^{11}};u=2-3x$
\end_inset


\begin_inset Formula 
\begin{align*}
\ds u & =-3\\
dx & =-\frac{1}{3}du
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
u & =2-3x\\
x & =-\frac{u-2}{3}\\
x & =\frac{2-u}{3}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\int xu^{11}\left(-\frac{1}{3}du\right) & =-\frac{1}{3}\int\left(\frac{2-u}{3}\right)\left(u^{11}\right)du\\
 & =-\frac{1}{9}\int2u^{11}-u^{12}du\\
 & =-\frac{1}{9}\left(\frac{2}{12}u^{12}-\frac{u^{13}}{13}\right)+c\\
 & =-\frac{1}{9}\left(\frac{1}{6}u^{12}-\frac{1}{13}u^{13}\right)+c\\
 & =-\frac{1}{54}\left(2-3x\right)^{12}+\frac{1}{117}\left(2-3x\right)^{13}+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\ix{\sin^{3}x};u=\cos x$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset space ~
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =-\sin x\\
dx & =\frac{du}{-\sin x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset space ~
\end_inset


\begin_inset Formula 
\begin{align*}
u & =\cos x\\
\frac{du}{dx} & =-\sin x\\
dx & =\frac{du}{-\sin x}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\ix{\sin^{3}x} & =\int\sin^{2}x\sin x\,dx\\
 & =\int\left(1-\cos^{2}x\right)\sin x\,dx\\
 & =\int\left(1-u^{2}\right)\sin x\frac{du}{-\sin x}\\
 & =-\int\left(1-u^{2}\right)\sin x\frac{du}{-\sin x}\\
 & =-\int\left(1-u^{2}\right)du\\
 & =-\left(u-\frac{u^{3}}{3}\right)+c\\
 & =-u+\frac{1}{3}u^{3}+c\\
 & =-u+\frac{1}{3}u^{3}+c\\
 & =-\cos x+\frac{1}{3}\cos^{3}x+c
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\int\sqrt{1-x^{2}}dx;x=\sin\theta$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $x=\sin\theta$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{dx}{d\theta} & =\cos\theta\\
dx & =\cos\theta d\theta
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\int\sqrt{1-x^{2}}dx$
\end_inset


\begin_inset Formula 
\begin{align*}
\int\sqrt{1-x^{2}}dx & =\int\sqrt{1-\left(\sin\theta\right)^{2}}\cos\theta d\theta\\
 & =\int\sqrt{\cos^{2}\theta}\cos\theta d\theta\\
 & =\int\cos^{2}\theta d\theta\\
 & =\int\frac{\cos2\theta+1}{2}d\theta\\
 & =\frac{1}{2}\left(\frac{\sin2\theta}{2}+\theta\right)+c\\
 & =\frac{1}{4}\sin2\theta+\frac{1}{2}\theta+c\\
 & =\frac{1}{4}\left(2\sin\theta\cos\theta\right)+\frac{1}{2}\theta+c\\
 & =\frac{1}{2}\left(\sin\theta\cos\theta+\theta\right)+c
\end{align*}

\end_inset


\end_layout

\end_deeper
