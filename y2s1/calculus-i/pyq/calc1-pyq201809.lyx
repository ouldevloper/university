\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $\int_{3}^{4}\frac{2xdx}{\left(x+4\right)\left(x-2\right)}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Partial fractions
\begin_inset Formula 
\begin{align*}
\frac{2x}{\left(x+4\right)\left(x-2\right)} & =\frac{A}{\left(x+4\right)}+\frac{B}{\left(x-2\right)}\\
2x & =A\left(x-2\right)+B\left(x+4\right)\\
A\left(x-2\right)+B\left(x+4\right)-2x & =0
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
When 
\begin_inset Formula $x=2$
\end_inset


\begin_inset Formula 
\begin{align*}
A\left(2-2\right)+B\left(2+4\right)-2\left(2\right) & =0\\
6B & =4\\
B & =\frac{4}{6}\\
B & =\frac{2}{3}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
When 
\begin_inset Formula $x=-4$
\end_inset


\begin_inset Formula 
\begin{align*}
A\left(-4-2\right)-2\left(-4\right) & =0\\
-6A+8 & =0\\
A & =\frac{8}{6}\\
 & =\frac{4}{3}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find equation
\begin_inset Formula 
\begin{align*}
\frac{2x}{\left(x+4\right)\left(x-2\right)} & =\frac{4}{3\left(x+4\right)}+\frac{2}{3\left(x-2\right)}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Differentiate
\begin_inset Formula 
\begin{align*}
\int_{3}^{4}\frac{2xdx}{\left(x+4\right)\left(x-2\right)} & =\int_{3}^{4}\frac{4}{3\left(x+4\right)}+\frac{2}{3\left(x-2\right)}dx\\
 & =\frac{4}{3}\int_{3}^{4}\frac{1}{\left(x+4\right)}dx+\frac{2}{3}\int_{3}^{4}\frac{1}{\left(x-2\right)}dx\\
 & =\frac{4}{3}\left[\ln\left(x+4\right)\right]_{3}^{4}+\frac{2}{3}\left[\ln\left(x-2\right)\right]_{3}^{4}\\
 & =\frac{4}{3}\left(\ln8-\ln7\right)+\frac{2}{3}\left(\ln2-\ln1\right)\\
 & =\frac{4}{3}\ln\frac{8}{7}+\frac{2}{3}\ln2\\
 & =0.6401
\end{align*}

\end_inset


\end_layout

\end_deeper
\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\int_{0}^{\frac{\pi}{4}}\left(1-\sin2x\right)^{\frac{3}{2}}\cos2xdx$
\end_inset

, Substitution
\begin_inset Formula 
\begin{align*}
u & =1-\sin2x\\
\frac{du}{dx} & =-2\cos2x\\
dx & =-\frac{du}{2\cos2x}
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Find bounds with respect to 
\begin_inset Formula $u$
\end_inset

 
\end_layout

\begin_deeper
\begin_layout Enumerate
When 
\begin_inset Formula $x=\frac{\pi}{4}$
\end_inset


\begin_inset Formula 
\begin{align*}
u & =1-\sin2\left(\frac{\pi}{4}\right)\\
 & =1-\sin\frac{\pi}{2}\\
 & =0
\end{align*}

\end_inset

When 
\begin_inset Formula $x=0$
\end_inset


\begin_inset Formula 
\begin{align*}
u & =1-\sin0\\
 & =1
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Substitute inside
\begin_inset Formula 
\begin{align*}
\int_{0}^{\frac{\pi}{4}}\left(1-\sin2x\right)^{\frac{3}{2}}\cos2xdx & =\int_{1}^{0}u^{\frac{3}{2}}\cos2x\left(-\frac{du}{2\cos2x}\right)\\
 & =-\frac{1}{2}\int_{1}^{0}u^{\frac{3}{2}}du\\
 & =-\frac{1}{2}\left[\frac{3}{2}u^{\frac{1}{2}}\right]_{1}^{0}\\
 & =-\frac{1}{2}\left(-\frac{3}{2}\right)\\
\int_{0}^{\frac{\pi}{4}}\left(1-\sin2x\right)^{\frac{3}{2}}\cos2xdx & =\frac{3}{4}
\end{align*}

\end_inset


\end_layout

\end_deeper
