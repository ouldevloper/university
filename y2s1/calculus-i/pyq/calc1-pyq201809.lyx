\begin_inset Formula 
\begin{align*}
f'\left(x\right) & =\lim_{h\to0}\frac{f\left(x+h\right)-f\left(x\right)}{h}\\
 & =\lim_{h\to0}\frac{3\left(x+h\right)^{2}-3-\left(3x^{2}-3\right)}{h}\\
 & =\lim_{h\to0}\frac{3\left(x^{2}+2hx+h^{2}\right)-3-3x^{2}+3}{h}\\
 & =\lim_{h\to0}\frac{3x^{2}+6hx+3h^{2}-3x^{2}}{h}\\
 & =\lim_{h\to0}\frac{h\left(6x+3h\right)}{h}\\
 & =\lim_{h\to0}6x+3h\\
f'\left(x\right) & =6x
\end{align*}

