\begin_layout Enumerate
\begin_inset Formula $\int_{0}^{1}\frac{xdx}{\left(x+2\right)\left(x+3\right)}$
\end_inset

(Partial fractions)
\end_layout

\begin_deeper
\begin_layout Enumerate
Solve partial fractions
\begin_inset Formula 
\begin{align*}
\frac{x}{\left(x+2\right)\left(x+3\right)} & =\frac{A}{\left(x+2\right)}+\frac{B}{\left(x+3\right)}\\
x & =A\left(x+3\right)+B\left(x+2\right)\\
A\left(x+3\right)+B\left(x+2\right)-x & =0
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
When 
\begin_inset Formula $x=-3$
\end_inset


\begin_inset Formula 
\begin{align*}
A\left(-3+3\right)+B\left(-3+2\right)-\left(-3\right) & =0\\
-B+3 & =0\\
B & =3
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
When 
\begin_inset Formula $x=-2$
\end_inset


\begin_inset Formula 
\begin{align*}
A\left(-2+3\right)+B\left(-2+2\right)-\left(-2\right) & =0\\
-A & =-2\\
A & =2
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find equation
\begin_inset Formula 
\[
\frac{x}{\left(x+2\right)\left(x+3\right)}=\frac{2}{x+2}+\frac{3}{x+3}
\]

\end_inset


\end_layout

\begin_layout Enumerate
Differentiate
\begin_inset Formula 
\begin{align*}
\int_{0}^{1}\frac{xdx}{\left(x+2\right)\left(x+3\right)} & =\int_{0}^{1}\left(\frac{2}{x+2}+\frac{3}{x+3}\right)dx\\
 & =\int_{0}^{1}\frac{2}{x+2}dx+\int_{0}^{1}\frac{3}{x+3}dx\\
 & =2\int_{0}^{1}\frac{1}{x+2}dx+3\int_{0}^{1}\frac{1}{x+3}dx\\
\int_{0}^{1}\frac{xdx}{\left(x+2\right)\left(x+3\right)} & =2\ln\left(x+2\right)+3\ln\left(x+3\right)
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\int_{0}^{1}x^{2}\sin\left(3x\right)dx$
\end_inset

 (Integration by parts)
\begin_inset Formula 
\begin{align*}
u & =x^{2}\\
v' & =\sin\left(3x\right)
\end{align*}

\end_inset


\begin_inset Formula 
\[
u'=2x
\]

\end_inset


\begin_inset Formula 
\begin{align*}
v & =\int\sin\left(3x\right)\\
v & =\frac{1}{3}\cos\left(3x\right)
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $\int uv'dx=uv-\int u'v\,dx$
\end_inset


\begin_inset Formula 
\begin{align*}
\int x^{2}\sin\left(3x\right)dx & =x^{2}\left(-\frac{1}{3}\cos\left(3x\right)\right)-\int2x\left(-\frac{1}{3}\cos\left(3x\right)\right)dx+c\\
 & =-\frac{1}{}x^{2}\cos\left(3x\right)+\frac{2}{3}\int x\cos\left(3x\right)dx+c\\
 & =-\frac{1}{3}x^{2}\cos\left(3x\right)+\frac{2}{3}\int x\cos\left(3x\right)dx+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Integrate by parts again
\begin_inset Formula 
\begin{align*}
u & =x\\
v' & =\cos\left(3x\right)\\
u' & =1\\
v & =\frac{1}{3}\sin\left(3x\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\int x\cos\left(3x\right)dx & =\frac{1}{3}x\sin\left(3x\right)-\left(\frac{1}{3}\right)\int\sin\left(3x\right)dx\\
 & =\frac{1}{3}x\sin\left(3x\right)-\frac{1}{3}\left(-\frac{1}{3}\right)\cos3x+c\\
 & =\frac{1}{3}x\sin\left(3x\right)+\frac{1}{9}\cos3x+c\\
 & =\frac{1}{9}\left(3\sin3x+\cos3x\right)+c
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Definite integration (remember radians)
\begin_inset Formula 
\begin{align*}
\int x^{2}\sin\left(3x\right)dx & =-\frac{1}{3}x^{2}\cos\left(3x\right)+\frac{2}{3}\left(\frac{1}{9}\left(3\sin3x+\cos3x\right)\right)+c\\
 & =-\frac{1}{3}x^{2}\cos\left(3x\right)+\frac{2}{27}\cos3x+\frac{2}{9}x\sin3x+c
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\int_{0}^{1}x^{2}\sin\left(3x\right)dx & =\left[-\frac{1}{3}x^{2}\cos\left(3x\right)+\frac{2}{27}\cos3x+\frac{2}{9}x\sin3x\right]_{0}^{1}\\
 & =0.21395\\
 & \approx0.214
\end{align*}

\end_inset


\end_layout

\end_deeper
