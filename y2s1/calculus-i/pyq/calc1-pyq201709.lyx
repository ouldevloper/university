\end_inset


\begin_inset Formula 
\begin{align*}
f'\left(x\right) & =\frac{d}{dx}\left[\frac{\sin x}{\sqrt{x-3}}\right]\\
 & =\frac{\sqrt{x-3}\frac{d}{dx}\left[\sin x\right]-\sin x\frac{d}{dx}\left[\sqrt{x-3}\right]}{x-3}\\
 & =\frac{\sqrt{x-3}\cos x-\sin x\frac{1}{2\sqrt{x-3}}}{x-3}\\
 & =\frac{\sqrt{x-3}\cos x-\frac{1}{2\sqrt{x-3}}\sin x}{x-3}\\
 & =\frac{\cos x}{\sqrt{x-3}}-\frac{\sin x}{2\left(x-3\right)^{\frac{3}{2}}}
\end{align*}

\end_inset


\end_layout

