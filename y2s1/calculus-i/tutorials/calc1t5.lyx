\begin_layout Enumerate
Find the equation of the tangent line to the curve 
\begin_inset Formula $y=\sec x-2\cos x$
\end_inset

 at the point (
\begin_inset Formula $\frac{\pi}{3}$
\end_inset

, 1).
\end_layout

\begin_deeper
\begin_layout Enumerate
The equation of a tangent line is 
\begin_inset Formula $y=mx+c$
\end_inset


\end_layout

\begin_layout Enumerate
First, find 
\begin_inset Formula $m$
\end_inset

, the gradient, or 
\begin_inset Formula $\frac{dy}{dx}$
\end_inset

, at point 
\begin_inset Formula $\left(\frac{\pi}{3},1\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
\dydx & =\sec x\tan x+2\sin x\\
 & =\sec\frac{\pi}{3}\tan\frac{\pi}{3}+2\sin\frac{\pi}{3}\\
 & =2\left(\sqrt{3}\right)+2\left(\frac{\sqrt{3}}{2}\right)\\
 & =2\sqrt{3}+\sqrt{3}\\
 & =3\sqrt{3}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Construct the complete equation
\begin_inset Formula 
\begin{align*}
y-1 & =3\sqrt{3}\left(x-\frac{\pi}{3}\right)\\
y & =3\sqrt{3}x-\sqrt{3}\pi+1
\end{align*}

\end_inset


\end_layout

