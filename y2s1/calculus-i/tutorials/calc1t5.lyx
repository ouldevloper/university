\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $y=\frac{5}{x^{2}}-\frac{x^{4}}{3}$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =\frac{5}{x^{2}}-\frac{x^{4}}{3}\\
 & =5x^{-2}-\frac{1}{3}x^{4}\\
\frac{dy}{dx} & =-10x^{-3}-\frac{4}{3}x^{3}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\sqrt{x^{3}}$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =x^{\frac{3}{2}}\\
\dydx & =\frac{3}{2}x^{\frac{1}{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{6}{\sqrt{x}}$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =6*\frac{1}{\sqrt{x}}\\
 & =6*x^{-\frac{1}{2}}\\
\dydx & =6*-\frac{1}{2}*x^{-\frac{3}{2}}\\
 & =-3x^{-\frac{3}{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{e^{3x}}{16}$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =\frac{1}{16}e^{3x}\\
\dydx & =\frac{3}{16}e^{\frac{3}{x}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=3\ln x$
\end_inset


\begin_inset Formula 
\begin{align*}
\dydx & =3*\frac{1}{x}\\
 & =\frac{3}{x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{x^{2}-1}{x}$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =x-\frac{1}{x}\\
 & =x-x^{-1}\\
\dydx & =1-\left(-1\right)*x^{-2}\\
 & =1+\frac{1}{x^{2}}
\end{align*}

\end_inset


\end_layout

\end_deeper
