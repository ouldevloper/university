\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $f\left(x\right)=x\sin x$
\end_inset


\begin_inset Formula 
\begin{align*}
f'\left(x\right) & =\frac{d}{dx}\left(x\right)*\sin x+x*\frac{d}{dx}\left(\sin x\right)\\
 & =\sin x+x*\left(\cos x\right)\\
 & =\sin x+x\cos x
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $g\left(t\right)=4\sec t+\tan t$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{dg}{dt} & =4\left(\sec x\tan x\right)+\sec^{2}x
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=e^{u}\left(\cos u+cu\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =1*e^{u}*\left(\cos u+cu\right)+\left(e^{u}\right)*\left(-\sin u+c\right)\\
 & =e^{u}\left(\cos u-\sin u+cu+c\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{\sin x}{1+\cos x}$
\end_inset


\begin_inset Formula 
\begin{align*}
\dydx & =\frac{\left(1+\cos x\right)\left(\cos x\right)-\left(\sin x\right)\left(-\sin x\right)}{\left(1+\cos x\right)^{2}}\\
 & =\frac{\cos x+\cos^{2}x+\sin^{2}x}{\left(1+\cos x\right)^{2}}\\
 & =\frac{\cos x+1}{\left(1+\cos x\right)^{2}},\text{note:}\text{\ensuremath{\sin^{2}x+\cos^{2}x=1}}\\
 & =\frac{1}{1+\cos x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{\tan x-1}{\sec x}$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =\frac{\tan x}{\sec x}-\frac{1}{\sec x}\\
 & =\frac{\frac{\sin x}{\cos x}}{\frac{1}{\cos x}}-\cos x\\
 & =\frac{\sin x}{\cos x}*\cos x-\cos x\\
 & =\sin x-\cos x\\
\dydx & =\cos x-\left(-\sin x\right)\\
 & =\sin x+\cos x
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Alternatively
\begin_inset Formula 
\begin{align*}
y & =\frac{\sec x\left(\sec^{2}x\right)-\left(\tan x-1\right)\left(\sec x\tan x\right)}{\left(\sec x\right)^{2}}\\
 & =\frac{\sec^{3}x+\left(1-\tan x\right)\left(\sec x\tan x\right)}{\left(\sec x\right)^{2}}\\
 & =\frac{\sec^{3}x+\left(\sec x\tan x-\sec x\tan^{2}x\right)}{\sec^{2}x}\\
 & =\frac{\sec^{2}x+\tan x-\tan^{2}x}{\sec x}\\
 & =\frac{1+\tan^{2}\left(x\right)+\tan x-\tan^{2}x}{\sec x};\text{Note:}\sec^{2}x=1+\tan^{2}\left(x\right)\\
 & =\frac{1+\tan x}{\sec x}\\
 & =\frac{\tan x+1}{\sec x}\\
 & =\frac{\tan x}{\sec x}+\frac{1}{\sec x}\\
 & =\sin x+\cos x
\end{align*}

\end_inset


\end_layout

\end_deeper
\end_deeper
