\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $y=x\sqrt{x+3}$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =x\left(x+3\right)^{\frac{1}{2}}\\
\dydx & =1\cdot\left(x+3\right)^{\frac{1}{2}}+x\cdot\frac{1}{2}\left(x+3\right)^{-\frac{1}{2}}\\
 & =\left(x+3\right)^{\frac{1}{2}}+\frac{x}{2}\left(x+3\right)^{-\frac{1}{2}}\\
 & =\left(x+3\right)^{-\frac{1}{2}}\left(x+3+\frac{1}{2}x\right)\\
 & =\left(x+3\right)^{-\frac{1}{2}}\left(\frac{3}{2}x+3\right)\\
 & =\frac{\frac{3}{2}x+3}{\sqrt{x+3}}\\
 & =\frac{\frac{3}{2}\left(x+2\right)}{\sqrt{x+3}}\\
 & =\frac{3\left(x+2\right)}{2\sqrt{x+3}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\sqrt{x}\cdot e^{x}$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =x^{\frac{1}{2}}e^{x}\\
\dydx & =\frac{1}{2}x^{-\frac{1}{2}}*e^{x}+x^{\frac{1}{2}}*e^{x}\\
 & =e^{x}x^{-\frac{1}{2}}\left(\frac{1}{2}+x\right)\\
 & =\frac{e^{x}\left(2x+1\right)}{2\sqrt{x}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\left(1+\sin x\right)\tan x$
\end_inset


\begin_inset Formula 
\begin{align*}
y & =\left(\cos x\right)\left(\tan x\right)+\left(1+\sin x\right)\left(\sec^{2}x\right)\\
 & =\left(\cos x\right)\left(\frac{\sin x}{\cos x}\right)+\left(1+\sin x\right)\left(\sec^{2}x\right)\\
 & =\sin x+\sec^{2}x+\tan x\sec x\\
 & =\sin x+\sec^{2}x+\frac{\tan x}{\cos x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\left(1-x^{3}\right)\left(1+x^{3}\right)$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $Ans:-6x^{5}$
\end_inset

, note, the book answer is wrong
\begin_inset Formula 
\begin{align*}
y & =-3x^{2}*\left(1+x^{3}\right)+\left(1-x^{3}\right)*3x^{2}\\
 & =-3x^{2}-3x^{5}+3x^{2}-3x^{5}\\
 & =-6x^{5}
\end{align*}

\end_inset


\end_layout

