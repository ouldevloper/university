\series bold
Quotient rule
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $y=\frac{2x^{2}+1}{x-1}$
\end_inset


\begin_inset Formula 
\begin{align*}
\dydx & =\frac{\left(x-1\right)\frac{d}{dx}\left(2x^{2}+1\right)-\left(2x^{2}+1\right)\frac{d}{dx}\left(x-1\right)}{\left(x-1\right)^{2}}\\
 & =\frac{\left(x-1\right)\left(4x\right)-\left(2x^{2}+1\right)}{\left(x-1\right)^{2}}\\
 & =\frac{4x^{2}-4x-2x^{2}-1}{\left(x-1\right)^{2}}\\
 & =\frac{2x^{2}-4x-1}{\left(x-1\right)^{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{e^{x}}{x^{3}+1}$
\end_inset


\begin_inset Formula 
\begin{align*}
\dydx & =\frac{\left(x^{3}+1\right)e^{x}-e^{x}\left(3x^{2}\right)}{\left(x^{3}+1\right)^{2}}\\
 & =\frac{\left(x^{3}+1\right)e^{x}-e^{x}\left(3x^{2}\right)}{\left(x^{3}+1\right)^{2}}\\
 & =e^{x}\left(\frac{x^{3}-3x^{2}+1}{\left(x^{3}+1\right)^{2}}\right)\\
 & =\frac{e^{x}\left(x^{3}-3x^{2}+1\right)}{\left(x^{3}+1\right)^{2}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $y=\frac{\cos x}{\sin x}$
\end_inset


\begin_inset Formula 
\begin{align*}
\dydx & =\frac{\sin x\cdot\left(-\sin x\right)-\cos x\cdot\left(\cos x\right)}{\sin^{2}x}\\
 & =\frac{-\sin^{2}x-\cos^{2}x}{\sin^{2}x}\\
 & =\frac{-1}{\sin^{2}x}\\
 & =-\csc^{2}x
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $G\left(r\right)=\sqrt{r}+\sqrt[3]{r}$
\end_inset


\begin_inset Formula 
\begin{align*}
G\left(r\right) & =r^{\frac{1}{2}}+r^{\frac{1}{3}}\\
G'\left(r\right) & =\frac{1}{2}r^{-\frac{1}{2}}+\frac{1}{3}r^{-\frac{2}{3}}\\
G''\left(r\right) & =-\frac{1}{4}r^{-\frac{3}{2}}-\frac{2}{9}r^{-\frac{5}{3}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
If 
\begin_inset Formula $F\left(x\right)=f\left(x\right)g\left(x\right)$
\end_inset


\begin_inset Formula 
\begin{align*}
F\left(x\right) & =f\left(x\right)g\left(x\right)\\
F'\left(x\right) & =f'\left(x\right)g\left(x\right)+f\left(x\right)g'\left(x\right)\\
F''\left(x\right) & =\frac{d}{dx}\left(f'\left(x\right)g\left(x\right)\right)+\frac{d}{dx}\left(f\left(x\right)g'\left(x\right)\right)\\
 & =f''\left(x\right)g\left(x\right)+f'\left(x\right)g'\left(x\right)+f'\left(x\right)g'\left(x\right)+f\left(x\right)g''\left(x\right)\\
 & =f''g\left(x\right)+2f'g'\left(x\right)+fg''\left(x\right)\\
 & =f''g+2f'g'+fg''
\end{align*}

\end_inset


\end_layout

