\begin_layout Enumerate
Find the derivative of the function using the definition of derivative.
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $f\left(x\right)=5-4x+3x^{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
f'\left(x\right) & =\lt h0{\frac{f\left(x+h\right)-f\left(x\right)}{h}}\\
 & =\lt h0{\frac{5-4\left(x+h\right)+3\left(x+h\right)^{2}-5+4x-3x^{2}}{h}}\\
 & =\lt h0{\frac{5-4x-4h+3\left(x^{2}+2hx+h^{2}\right)-5+4x-3x^{2}}{h}}\\
 & =\lt h0{\frac{\cancel{5-5-4x+4x+3x^{2}-3x^{2}}+3h^{2}+6hx-4h}{h}}\\
 & =\lt h0{\frac{3h^{2}+6hx-4h}{h}}\\
 & =\lt h0{\frac{\cancel{h}\left(3h+6x-4\right)}{\cancel{h}}}\\
 & =\lt h0{\left(3h+6x-4\right)}\\
\boldsymbol{f'\left(x\right)} & \boldsymbol{=6x-4}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $f\left(x\right)=\frac{3}{x-2}$
\end_inset


\begin_inset Formula 
\begin{align*}
f'\left(x\right) & =\lim_{h\to0}\frac{f\left(x+h\right)-f\left(x\right)}{h}\\
 & =\lim_{h\to0}\frac{\frac{3}{x+h-2}-\frac{3}{x-2}}{h}\\
 & =\lim_{h\to0}\left(\frac{3}{x+h-2}*\frac{x-2}{x-2}-\frac{3}{x-2}*\frac{x+h-2}{x+h-2}\right)*\frac{1}{h}\\
 & =\lim_{h\to0}\frac{3x-6-3x-3h+6}{\left(x+h-2\right)\left(x-2\right)}*\frac{1}{h}\\
 & =\lim_{h\to0}\frac{\cancel{3x-3x}-3\cancel{h}\cancel{+6-6}}{\left(x+h-2\right)\left(x-2\right)}*\frac{1}{\cancel{h}}\\
 & =\lim_{h\to0}\frac{-3}{\left(x+h-2\right)\left(x-2\right)}\\
 & =\frac{-3}{\left(x-2\right)\left(x-2\right)}\\
\boldsymbol{f'\left(x\right)} & \boldsymbol{=-\frac{3}{\left(x-2\right)^{2}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $f\left(x\right)=\sqrt{3x+1}$
\end_inset


\begin_inset Formula 
\begin{align*}
f'\left(x\right) & =\lim_{h\to0}\frac{f\left(x+h\right)-f\left(x\right)}{h}\\
 & =\lim_{h\to0}\frac{\sqrt{3\left(x+h\right)+1}-\sqrt{3x+1}}{h}\\
 & =\lim_{h\to0}\frac{\sqrt{3\left(x+h\right)+1}-\sqrt{3x+1}}{h}*\frac{\sqrt{3\left(x+h\right)+1}+\sqrt{3x+1}}{\sqrt{3\left(x+h\right)+1}+\sqrt{3x+1}}\\
 & =\lim_{h\to0}\frac{3\left(x+h\right)+1-\left(3x+1\right)}{h\sqrt{3\left(x+h\right)+1}+\sqrt{3x+1}}\\
 & =\lim_{h\to0}\frac{\cancel{3x-3x+1-1}+3h}{h\sqrt{3\left(x+h\right)+1}+\sqrt{3x+1}}\\
 & =\lim_{h\to0}\frac{3\cancel{h}}{\cancel{h}\sqrt{3\left(x+h\right)+1}+\sqrt{3x+1}}\\
 & =\frac{3}{\sqrt{3x+1}+\sqrt{3x+1}}\\
\boldsymbol{f'\left(x\right)} & \boldsymbol{=\frac{3}{2\sqrt{3x+1}}}
\end{align*}

\end_inset


