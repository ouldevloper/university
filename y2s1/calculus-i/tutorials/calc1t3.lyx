\begin_inset Formula 
\begin{align*}
\lim_{x\to1}\frac{x^{3}-1}{x^{2}-1} & =\lim_{x\to1}\frac{x^{3}-1^{3}}{x^{2}-1}\\
 & =\lim_{x\to1}\frac{\left(x+\left(-1\right)\right)\left(x^{2}-\left(x\right)\left(-1\right)+\left(-1\right)^{2}\right)}{\left(x-1\right)\left(x+1\right)}\\
 & =\lim_{x\to1}\frac{\left(x-1\right)\left(x^{2}+x+1\right)}{\left(x-1\right)\left(x+1\right)}\\
 & =\lim_{x\to1}\frac{\cancel{\left(x-1\right)}\left(x^{2}+x+1\right)}{\cancel{\left(x-1\right)}\left(x+1\right)}\\
 & =\frac{\left(\left(1\right)^{2}+\left(1\right)+1\right)}{\left(\left(1\right)+1\right)}\\
\boldsymbol{\lim_{x\to1}\frac{x^{3}-1}{x^{2}-1}} & \boldsymbol{=\frac{3}{2}}
\end{align*}

