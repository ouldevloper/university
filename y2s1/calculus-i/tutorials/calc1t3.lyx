\begin_inset Formula $\frac{1}{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
\lx 0\frac{\sqrt{1+h}-1}{h} & =\lx 0\frac{\sqrt{1+h}-1}{h}*\frac{\sqrt{1+h}+1}{\sqrt{1+h}+1}\\
 & =\lx 0\frac{\sqrt{1+h}-1\left(\sqrt{1+h}+1\right)}{h\left(\sqrt{1+h}+1\right)}\\
 & =\lx 0\frac{1+h-1}{h\left(\sqrt{1+h}+1\right)}\\
 & =\lx 0\frac{h}{h\left(\sqrt{1+h}+1\right)}\\
 & =\lx 0\frac{1}{\sqrt{1+h}+1}\\
 & =\frac{1}{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\lim_{x\to2}\frac{x^{4}-16}{x-2}$
\end_inset


\begin_inset Formula 
\begin{align*}
\lim_{x\to2}\frac{x^{4}-16}{x-2} & =\lim_{x\to2}\frac{\left(x^{2}-4\right)\left(x^{2}+4\right)}{x-2}\\
 & =\lim_{x\to2}\frac{\cancel{\left(x-2\right)}\left(x+2\right)\left(x^{2}+4\right)}{\cancel{x-2}}\\
 & =\lim_{x\to2}\left(x+2\right)\left(x^{2}+4\right)\\
 & =\left(2+2\right)\left(2^{2}+4\right)\\
 & =32
\end{align*}

\end_inset


\end_layout

