R_{f} & =\mathbb{R}-\left\{ 0\right\} 
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
D_{g} & =\mathbb{R}-\left\{ -2\right\} \\
 & =\mathbb{R}-\left\{ 0\right\} 
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
f\circ g & =f\left(\frac{x+1}{x+2}\right)\\
 & =\left(\frac{x+1}{x+2}\right)+\frac{1}{\left(\frac{x+1}{x+2}\right)}\\
 & =\left(\frac{x+1}{x+2}\right)+\left(\frac{x+2}{x+1}\right)\\
 & =\frac{\left(x+1\right)\left(x+1\right)+\left(x+2\right)\left(x+2\right)}{\left(x+2\right)\left(x+1\right)}\\
 & =\frac{\left(x+1\right)^{2}+\left(x+2\right)^{2}}{\left(x+2\right)\left(x+1\right)}\\
 & =\frac{x^{2}+2x+1+x^{2}+4x+2}{x^{2}+3x+2}\\
 & =\frac{2x^{2}+6x+3}{x^{2}+3x+2}
\end{align*}

