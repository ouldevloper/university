\begin_inset Formula 
\begin{align*}
f+g & =\sqrt{x+1}+\sqrt{1-x}\\
D\left(f+g\right) & =D\left(f\right)\cap D\left(g\right)\\
 & =-1\le x\le1
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset space ~
\end_inset


\begin_inset Formula 
\begin{align*}
f-g & =\sqrt{x+1}-\sqrt{1-x}\\
D\left(f-g\right) & =-1\le x\le1
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset space ~
\end_inset


\begin_inset Formula 
\begin{align*}
fg & =\sqrt{x+1}\cdot\sqrt{1-x}\\
 & =\sqrt{\left(x+1\right)\left(1-x\right)}\\
 & =\sqrt{1-x^{2}}\\
 & =[-1,1]
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset space ~
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{f}{g} & =\frac{\sqrt{x+1}}{\sqrt{1-x}}\\
 & =\sqrt{\frac{x+1}{1-x}}\\
 & =-1\le x<1,
\end{align*}

\end_inset


\end_layout

