\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $\int\frac{2x+1}{\left(x-3\right)^{6}}dx;u=x-3$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Find 
\begin_inset Formula $dx$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =1\\
du & =dx
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute in
\begin_inset Formula 
\begin{align*}
\int\frac{2\left(x+3-3\right)+1}{u^{6}}du & =\int\frac{2\left(u+3\right)+1}{u^{6}}du\\
 & =\int\frac{2u+6+1}{u^{6}}du\\
 & =\int\frac{2u}{u^{6}}du+\int\frac{6}{u^{6}}du+\int\frac{1}{u^{6}}du\\
 & =\int2u^{-5}du+\int6u^{-6}du+\int u^{-6}du\\
 & =\left(-\frac{2}{4}u^{-4}\right)+6\left(-\frac{1}{5}u^{-5}\right)+\left(-\frac{1}{5}u^{-5}\right)+c\\
 & =-\frac{1}{2u^{4}}-\frac{6}{5u^{5}}-\frac{1}{5u^{-5}}+c\\
 & =-\frac{1}{2\left(x-3\right)^{4}}-\frac{7}{5\left(x-3\right)^{5}}+c
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset space ~
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $\int x^{3}\sqrt{1+x^{2}}dx;u=\sqrt{1+x^{2}}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Find 
\begin_inset Formula $dx$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =\frac{1}{2}\left(1+x^{2}\right)^{-\frac{1}{2}}\left(2x\right)\\
 & =\frac{x}{\sqrt{1+x^{2}}}\\
du & =\frac{x}{\sqrt{1+x^{2}}}dx\\
dx & =\frac{\left(1+x^{2}\right)^{\frac{1}{2}}}{x}du
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
u & =\sqrt{1+x^{2}}\\
u^{2} & =1+x^{2}\\
x & =\sqrt{u^{2}-1}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute in
\begin_inset Formula 
\begin{align*}
\int x^{3}\sqrt{1+x^{2}}dx & =\int x^{3}\sqrt{1+x^{2}}\frac{\left(1+x^{2}\right)^{\frac{1}{2}}}{x}du\\
 & =\int x^{2}\left(1+x^{2}\right)du\\
 & =\int\left(u^{2}-1\right)u^{2}du\\
 & =\int u^{4}-u^{2}du\\
 & =\frac{u^{5}}{5}-\frac{u^{3}}{3}+c\\
 & =\frac{\left(1+x^{2}\right)^{\frac{5}{2}}}{5}-\frac{\left(1+x^{2}\right)^{\frac{3}{2}}}{3}+c
\end{align*}

\end_inset


\end_layout

\end_deeper
