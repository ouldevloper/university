\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $\int\frac{x-9}{\left(x+5\right)\left(x-2\right)}dx$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Part-ify the equation
\begin_inset Formula 
\begin{align*}
\frac{x-9}{\left(x+5\right)\left(x-2\right)} & =\frac{A}{\left(x+5\right)}+\frac{B}{\left(x-2\right)}\\
 & =\frac{A\left(x-2\right)+B\left(x+5\right)}{\left(x+5\right)\left(x-2\right)}\\
x-9 & =A\left(x-2\right)+B\left(x+5\right)\\
A\left(x-2\right)+B\left(x+5\right)-x+9 & =0
\end{align*}

\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
When 
\begin_inset Formula $x=2$
\end_inset


\begin_inset Formula 
\begin{align*}
A\left(2-2\right)+B\left(2+5\right)-2+9 & =0\\
7B+7 & =0\\
B & =-\frac{7}{7}\\
 & =-1
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
When 
\begin_inset Formula $x=-5$
\end_inset


\begin_inset Formula 
\begin{align*}
A\left(-5-2\right)+B\left(-5+5\right)-\left(-5\right)+9 & =0\\
-7A+5+9 & =0\\
-7A & =-14\\
A & =2
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Form the final equation
\begin_inset Formula 
\begin{align*}
\frac{x-9}{\left(x+5\right)\left(x-2\right)} & =\frac{A}{\left(x+5\right)}+\frac{B}{\left(x-2\right)}\\
 & =\frac{2}{\left(x+5\right)}-\frac{1}{\left(x-2\right)}
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Integrate the equations
\begin_inset Formula 
\begin{align*}
\int\frac{x-9}{\left(x+5\right)\left(x-2\right)}dx & =\int\frac{2}{\left(x+5\right)}-\frac{1}{\left(x-2\right)}dx\\
 & =2\int\frac{1}{x+5}dx-\int\frac{1}{x-2}dx\\
\int\frac{x-9}{\left(x+5\right)\left(x-2\right)}dx & =2\ln\left|\left(x+5\right)\right|-\ln\left|\left(x-2\right)\right|+c
\end{align*}

\end_inset


\end_layout

\end_deeper
