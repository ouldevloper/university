\begin_layout Enumerate
\begin_inset Formula $\int\cos^{2}x\sin^{3}xdx;u=\cos x$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Find 
\begin_inset Formula $dx$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =-\sin x\\
dx & =-\frac{du}{\sin x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find 
\begin_inset Formula $x$
\end_inset

 in terms of 
\begin_inset Formula $u$
\end_inset

 
\begin_inset Formula 
\begin{align*}
u & =\cos x
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute in
\begin_inset Formula 
\begin{align*}
\int\cos^{2}x\sin^{3}x\,dx & =\int u^{2}\sin^{3}x\left(-\frac{du}{\sin x}\right)\\
 & =-\int u^{2}\sin^{2}x\,du\\
 & =-\int u^{2}\left(1-\cos^{2}\left(x\right)\right)\,du\\
 & =-\int u^{2}\left(1-u^{2}\right)du\\
 & =-\int u^{2}-u^{4}du\\
 & =-\left[\frac{u^{3}}{3}-\frac{u^{5}}{5}\right]+c\\
 & =-\frac{\cos^{3}x}{3}-\frac{\cos^{5}x}{5}+c
\end{align*}

\end_inset


\end_layout

\end_deeper
