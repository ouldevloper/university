\begin_inset Formula $u=1+e^{x}$
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{du}{dx} & =e^{x}\\
du & =e^{x}dx\\
dx & =\frac{du}{e^{x}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substiute in
\begin_inset Formula 
\begin{align*}
\int e^{x}\sqrt{u}\frac{du}{e^{x}} & =\int\sqrt{u}du\\
 & =\int u^{\frac{1}{2}}du\\
 & =\frac{u^{\frac{3}{2}}}{\frac{3}{2}}+c\\
\int e^{x}\sqrt{1+e^{x}}dx & =\frac{2}{3}\left(1+e^{x}\right)^{\frac{3}{2}}+c
\end{align*}

