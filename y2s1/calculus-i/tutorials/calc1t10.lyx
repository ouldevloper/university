\begin_inset Formula $u=x^{2}$
\end_inset


\begin_inset Formula 
\begin{align*}
du & =2x\,dx\\
dx & =\frac{du}{2x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Substitute in 
\begin_inset Formula 
\begin{align*}
\int xe^{u}\frac{du}{2x} & =\int e^{u}\frac{du}{2}\\
 & =\frac{1}{2}\int e^{u}du\\
 & =\frac{1}{2}e^{u}+c\\
 & =\frac{1}{2}e^{x^{2}}+c
\end{align*}

\end_inset


