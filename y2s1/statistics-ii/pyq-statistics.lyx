\begin_layout Enumerate
\begin_inset Formula $X\sim P_{o}\left(\lambda=3\right)$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $X$
\end_inset

 be the number of customers coming to unisex hair salon in an hour.
\begin_inset Formula 
\begin{align*}
P\left(X>1\right) & =1-P\left(X=0\right)-P\left(X=1\right)\\
 & =1-\frac{e^{-3}3^{0}}{0!}-\frac{e^{-3}3}{1!}\\
 & =1-0.04979-0.1494\\
 & =0.8008
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
