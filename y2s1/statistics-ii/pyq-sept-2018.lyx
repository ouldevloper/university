\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
T & =\frac{\left(66.1-68.5\right)-0}{3.5341*\sqrt{\frac{1}{9}+\frac{1}{16}}}\\
 & =-1.630
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Conclusion
