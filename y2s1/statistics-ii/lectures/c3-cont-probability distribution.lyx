c & =\frac{1}{b-a}\\
 & =\frac{1}{5-1}\\
 & =\frac{1}{4}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
P(2<X<4.6) & =\int_{2}^{4.6}\frac{1}{4}dx\\
 & =\frac{1}{4}x]_{2}^{4.6}\\
