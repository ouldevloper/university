\begin_layout Enumerate
Find the least square regression line
\begin_inset Formula 
\begin{align*}
\hat{b} & =\frac{n\sum XY-\left(\sum X\right)\left(\sum Y\right)}{n\sum X^{2}-\left(\sum X\right)^{2}}\\
 & =\frac{\left(10\right)\left(97650\right)-\left(300\right)\left(2970\right)}{\left(10\right)\left(12050\right)-\left(300\right)^{2}}\\
 & =2.8033
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\hat{a} & =\frac{\sum Y}{n}-\hat{b}\frac{\sum X}{n}\\
 & =\frac{2970}{10}-2.8033\left(\frac{300}{10}\right)\\
 & =212.90
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\hat{Y} & =212.9+2.8033X
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
When X = 40
\begin_inset Formula 
\begin{align*}
\hat{Y} & =212.90+2.8033\left(40\right)\\
 & =325.032
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Conclusion
\end_layout

