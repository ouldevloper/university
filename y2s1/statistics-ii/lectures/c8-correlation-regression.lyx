\begin_layout Enumerate
Product moment correlation coefficient
\begin_inset Formula 
\begin{align*}
r & =\frac{n\sum XY-\left(\sum X\right)\left(\sum Y\right)}{\sqrt{\left[n\sum X^{2}-\left(\sum X\right)^{2}\right]\left[n\sum Y^{2}-\left(\sum Y\right)^{2}\right]}}\\
 & =\frac{8\left(154650\right)-\left(290\right)\left(3370\right)}{\sqrt{\left[8\left(12600\right)-\left(290\right)^{2}\right]\left[8\left(1950500\right)-\left(3370\right)^{2}\right]}}\\
r & =0.9759
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Equation of the regression line
\begin_inset Formula 
\begin{align*}
\hat{b} & =\frac{n\sum XY-\left(\sum X\right)\left(\sum Y\right)}{n\sum X^{2}-\left(\sum X\right)^{2}}\\
 & =\frac{\left(8\right)\left(154650\right)-\left(290\right)\left(3370\right)}{\left(8\right)\left(12600\right)-\left(290\right)^{2}}\\
 & =15.5629
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\hat{a} & =\frac{\sum Y}{n}-\hat{b}\frac{\sum X}{n}\\
 & =\frac{3370}{8}-\left(15.5629\right)\frac{290}{8}\\
 & =-142.9051
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\hat{Y} & =\hat{a}+\hat{b}X\\
 & =15.5629X-142.9051
\end{align*}

\end_inset


\end_layout

