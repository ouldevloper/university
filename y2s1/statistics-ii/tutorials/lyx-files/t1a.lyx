\begin_layout Itemize
Average number of hours
\begin_inset Formula 
\begin{align*}
\mu & =\int_{0}^{1}x\cdot x\,dx+\int_{1}^{2}x\cdot\left(2-x\right)dx\\
 & =\left[\frac{x^{3}}{3}\right]_{0}^{1}+\int_{1}^{2}2x-x^{2}dx\\
 & =\left[\frac{x^{3}}{3}\right]_{0}^{1}+\left[x^{2}-\frac{x^{3}}{3}\right]_{1}^{2}\\
 & =\frac{1}{3}+\left(4-\frac{2^{3}}{3}\right)-\left(1-\frac{1}{3}\right)\\
 & =1\\
 & =100hours
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
Variance
\begin_inset Formula 
\begin{align*}
\sigma^{2} & =(\int_{0}^{1}x^{2}\cdot x\,dx+\int_{1}^{2}x^{2}\cdot\left(2-x\right)dx)-\mu^{2}\\
 & =\left[\frac{x^{4}}{4}\right]_{0}^{1}+\left[\frac{2}{3}x^{3}-\frac{x^{4}}{4}\right]_{1}^{2}-\left(1\right)^{2}\\
 & =\frac{1}{4}+\left(\frac{2}{3}\left(2\right)^{3}-\frac{\left(2\right)^{4}}{4}-\left(\frac{2}{3}-\frac{1}{4}\right)\right)-\left(1\right)\\
 & =\frac{1}{6}\\
 & =16\frac{2}{3}hours
\end{align*}

