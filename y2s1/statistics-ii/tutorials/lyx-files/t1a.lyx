\begin_layout Itemize
Mean
\begin_inset Formula 
\begin{align*}
\mu & =\int_{-\infty}^{\infty}x\cdot f\left(x\right)dx\\
 & =\int_{1}^{2}2x\left(x-1\right)dx\\
 & =2\int_{1}^{2}x^{2}-x\,dx\\
 & =2\left[\frac{x^{3}}{3}-\frac{x^{2}}{2}\right]_{1}^{2}\\
 & =2\left(\frac{2^{3}}{3}-\frac{2^{2}}{2}-\left(\frac{1^{3}}{3}-\frac{1^{2}}{2}\right)\right)\\
 & =\frac{5}{3}litres
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
Variance
\begin_inset Formula 
\begin{align*}
\sigma^{2} & =\int_{-\infty}^{\infty}x^{2}\cdot f\left(x\right)dx-\mu^{2}\\
 & =2\int_{1}^{2}x^{2}\left(x-1\right)dx-\left(\frac{5}{3}\right)^{2}\\
 & =2\left[\frac{x^{4}}{4}-\frac{x^{3}}{3}\right]_{1}^{2}-\left(\frac{5}{3}\right)^{2}\\
 & =2\left(\frac{2^{4}}{4}-\frac{2^{3}}{3}-\left(\frac{1^{4}}{4}-\frac{1^{3}}{3}\right)\right)-\left(\frac{5}{3}\right)^{2}\\
 & =\frac{1}{18}litres
\end{align*}

\end_inset


\end_layout

\begin_layout Paragraph
Q11
\end_layout

\begin_layout Itemize
Mean
\begin_inset Formula 
\begin{align*}
\mu & =\int_{0}^{1}x\frac{2}{5}\left(x+2\right)dx\\
 & =\frac{2}{5}\int_{0}^{1}x^{2}+2x\,dx\\
 & =\frac{2}{5}\left[\frac{x^{3}}{3}+x^{2}\right]_{0}^{1}\\
 & =\frac{2}{5}\left[\frac{1}{3}+1\right]\\
 & =\frac{2}{5}\left[\frac{4}{3}\right]\\
 & =\frac{8}{15}people
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
Variance
\begin_inset Formula 
\begin{align*}
\sigma^{2} & =\int_{0}^{1}x^{2}\frac{2}{5}\left(x+2\right)dx-\mu^{2}\\
 & =\frac{2}{5}\int_{0}^{1}x^{2}\left(x+2\right)dx-\left(\frac{8}{15}\right)^{2}\\
 & =\frac{2}{5}\int_{0}^{1}x^{3}+2x^{2}dx-\left(\frac{8}{15}\right)^{2}\\
 & =\frac{2}{5}\left[\frac{x^{4}}{4}+\frac{2}{3}x^{3}\right]_{0}^{1}-\left(\frac{8}{15}\right)^{2}\\
 & =\frac{2}{5}\left[\frac{1}{4}+\frac{2}{3}\right]-\left(\frac{8}{15}\right)^{2}\\
 & =\frac{37}{450}people
\end{align*}

\end_inset


\end_layout

\begin_layout Paragraph
