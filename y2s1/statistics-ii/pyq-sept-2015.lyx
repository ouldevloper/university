\begin_inset Formula 
\begin{align*}
\mu & =\int_{0}^{1}xf\left(x\right)dx\\
 & =12\int_{0}^{1}x\left(x^{2}-x^{3}\right)dx\\
 & =12\int_{0}^{1}x^{3}-x^{4}dx\\
 & =\frac{3}{5}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\sigma^{2} & =\int_{0}^{1}x^{2}f\left(x\right)dx-\mu^{2}\\
 & =12\int_{0}^{1}x^{2}\left(x^{2}-x^{3}\right)dx-\frac{3}{5}^{2}\\
 & =12\int_{0}^{1}x^{4}-x^{5}dx-\frac{3}{5}^{2}\\
 & =\frac{1}{25}\\
\sigma & =\sqrt{\frac{1}{25}}\\
 & =\frac{1}{5}
\end{align*}

