\begin_inset Formula 
\begin{align*}
\int_{0}^{1}kx^{2}\left(1-x\right)dx & =1\\
k\int_{0}^{1}x^{2}-x^{3}dx & =1\\
k\left[\frac{x^{3}}{3}-\frac{x^{4}}{4}\right]_{0}^{1} & =1\\
\left[\frac{x^{3}}{3}-\frac{x^{4}}{4}\right]_{0}^{1} & =\frac{1}{k}\\
\frac{1}{3}-\frac{1}{4} & =\frac{1}{k}\\
\frac{1}{12} & =\frac{1}{k}\\
k & =12
\end{align*}

\end_inset


