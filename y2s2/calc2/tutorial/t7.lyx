\begin_layout Enumerate
\begin_inset Formula $a_{n}=\frac{e^{n}+e^{-n}}{e^{2n}-1}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Converges
\begin_inset Formula 
\begin{align*}
a_{n} & =\frac{e^{n}+e^{-n}}{e^{2n}-1}\\
 & =\lim_{n\to\infty}\frac{\frac{e^{n}}{e^{2n}}+\frac{e^{-n}}{e^{2n}}}{\frac{e^{2n}}{e^{2n}}-\frac{1}{e^{2n}}}\\
 & =\lim_{n\to\infty}\frac{\frac{1}{e^{n}}+\frac{1}{e^{3n}}}{1-\frac{1}{e^{2n}}}\\
a_{n} & =0
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $a_{n}=\frac{\left(n+2\right)!}{n!}$
\end_inset


\end_layout

\begin_deeper
