\begin_layout Enumerate
\begin_inset Formula $a_{n}=\frac{\sin^{2}n}{2^{n}}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Proof (Squeeze theorem)
\begin_inset Formula 
\begin{align*}
-1 & \le\sin n\le1\\
0 & \le\sin^{2}n\le1\\
0 & \le\frac{\sin^{2}n}{2^{n}}\le\frac{1}{2^{n}}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\lim_{n\to\infty}0 & \le\lim_{n\to\infty}\frac{\sin^{2}n}{2^{n}}\le\lim_{n\to\infty}\frac{1}{2^{n}}\\
0 & \le\lim_{n\to\infty}\frac{\sin^{2}n}{2^{n}}\le0
\end{align*}

\end_inset


\begin_inset Formula 
\[
\lim_{n\to\infty}\frac{\sin^{2}n}{2^{n}}=0
\]

\end_inset


\end_layout

\begin_layout Enumerate
