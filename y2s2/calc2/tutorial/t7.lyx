\begin{align*}
\lim_{n\to\infty}a_{n} & =\lim_{n\to\infty}\frac{2^{n}}{3^{n+1}}\\
 & =\lim_{n\to\infty}\frac{2^{n}}{3^{n+1}}\\
 & =\frac{2^{\infty}}{3^{\infty+1}}\\
 & =0
\end{align*}

\end_inset


\end_layout

\end_deeper
