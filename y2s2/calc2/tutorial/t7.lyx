\begin_layout Enumerate
Determine whether the sequence converges or diverges.
 If it converges, find the limit.
 
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $a_{n}=\frac{2n+4n^{2}}{6n^{2}-1}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Converges
\begin_inset Formula 
\begin{align*}
\lim_{n\to\infty}a_{n} & =\lim_{n\to\infty}\frac{2n+4n^{2}}{6n^{2}-1}\\
 & =\lim_{n\to\infty}\frac{\frac{2n}{n^{2}}+\frac{4n^{2}}{n^{2}}}{\frac{6n^{2}}{n^{2}}-\frac{1}{n^{2}}}\\
 & =\lim_{n\to\infty}\frac{\frac{2}{n}+4}{6-\frac{1}{n^{2}}}\\
 & =\frac{4}{6}\\
 & =\frac{2}{3}
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $a_{n}=\frac{3n^{2}-2n+3}{4+n^{2}}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Converges
\end_layout

\begin_layout Enumerate
Limit
\begin_inset Formula 
\begin{align*}
a_{n} & =\lim_{n\to\infty}\frac{\frac{3n^{2}}{n^{2}}-\frac{2n}{n^{2}}+\frac{3}{n^{2}}}{\frac{4}{n^{2}}+\frac{n^{2}}{n^{2}}}\\
 & =\lim_{n\to\infty}\frac{3-\frac{2}{n}+\frac{3}{n^{2}}}{\frac{4}{n^{2}}+1}\\
 & =\frac{3}{1}\\
a_{n} & =3
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $a_{n}=n\left(n-1\right)$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Proof
\begin_inset Formula 
\begin{align*}
\lim_{n\to\infty}n\left(n-1\right) & =\infty\cdot\infty\\
 & =\infty
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Diverges
\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $a_{n}=\frac{\ln n^{2}}{n}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Converges
\begin_inset Formula 
\begin{align*}
\lim_{n\to\infty}a_{n} & =\lim_{n\to\infty}\frac{1}{n}\ln n^{2}\\
 & =0\cdot\ln\infty^{2}\\
\lim_{n\to\infty}a_{n} & =0
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $a_{n}=\frac{2^{n}}{3^{n+1}}$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Converges
