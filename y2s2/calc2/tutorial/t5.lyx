\begin_layout Enumerate
\begin_inset Formula $x^{2}\dydx+xy=x+1$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Put 
\begin_inset Formula $D.E.$
\end_inset

 in the correct standard form, coefficient of 
\begin_inset Formula $\frac{dy}{dx}$
\end_inset

 is 1.
\begin_inset Formula 
\begin{align*}
x^{2}\dydx+xy & =x+1\\
\frac{1}{x^{2}}\left[x^{2}\dydx+xy\right] & =\frac{1}{x^{2}}\left[x+1\right]\\
\boldsymbol{\frac{dy}{dx}+\frac{y}{x}} & \boldsymbol{=\frac{1}{x}+\frac{1}{x^{2}},p\left(x\right)=\frac{1}{x}}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Find the integrating factor 
\begin_inset Formula $\mu\left(x\right)=e^{\int p\left(x\right)dx}$
\end_inset

.
\begin_inset Formula 
\begin{align*}
\mu\left(x\right) & =e^{\int\frac{1}{x}dx}\\
 & =e^{\ln x}\\
\boldsymbol{\mu\left(x\right)} & \boldsymbol{=x}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Multiply everything in the D.E.
 by 
\begin_inset Formula $\mu\left(x\right)$
\end_inset

 , and verify that the left side becomes the product rule 
\begin_inset Formula $\frac{d}{dx}\left[\mu\left(x\right)y\right]$
\end_inset

 and write it as such.
\begin_inset Formula 
\begin{align*}
x\left[\frac{dy}{dx}+\frac{y}{x}\right] & =x\left[\frac{1}{x}+\frac{1}{x^{2}}\right]\\
x\frac{dy}{dx}+y & =1+\frac{1}{x}\\
\frac{d}{dx}\left[yx\right] & =1+\frac{1}{x}\\
\int\frac{d}{dx}\left[yx\right]dx & =\int1+\frac{1}{x}dx\\
yx & =x+\ln x+c\\
y & =1+\frac{\ln x+c}{x}\\
 & =1+\frac{\ln x}{x}+\frac{c}{x}
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
\begin_inset Formula $\dydx+y\cot x=\csc x$
\end_inset


\end_layout

\begin_deeper
\begin_layout Enumerate
Put 
\begin_inset Formula $D.E.$
\end_inset

 in the correct standard form, coefficient of 
\begin_inset Formula $\frac{dy}{dx}$
\end_inset

 is 1.
\end_layout

\begin_deeper
\begin_layout Enumerate
Already in correct standard form, 
\begin_inset Formula $p\left(x\right)=\cot x$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Find the integrating factor 
\begin_inset Formula $\mu\left(x\right)=e^{\int p\left(x\right)dx}$
\end_inset


\begin_inset Formula 
\begin{align*}
\mu\left(x\right) & =e^{\int\cot x\,dx}\\
 & =e^{\ln\left|\sin x\right|}\\
 & =\sin x
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Multiply everything in the D.E.
 by 
\begin_inset Formula $\mu\left(x\right)$
\end_inset

 , and verify that the left side becomes the product rule 
\begin_inset Formula $\frac{d}{dx}\left[\mu\left(x\right)y\right]$
\end_inset

 and write it as such.
\begin_inset Formula 
\begin{align*}
\sin x\left[\dydx+y\cot x\right] & =\sin x\left[\csc x\right]\\
\sin x\dydx+y\sin x\cdot\frac{\cos x}{\sin x} & =\sin x\left[\frac{1}{\sin x}\right]\\
\sin x\dydx+y\cos x & =1\\
\sin x\dydx+y\cos x & =1\\
\frac{d}{dx}\left[y\sin x\right] & =1\\
\int\frac{d}{dx}\left[y\sin x\right]dx & =\int1dx\\
y\sin x+c & =x+c\\
y & =\frac{x+c}{\sin x}
\end{align*}

\end_inset


\end_layout

\end_deeper
