\begin_inset Formula 
\begin{align*}
\frac{d}{dx}\left[y\right] & =-3ce^{-3x}+\frac{1}{3}\\
y' & =\frac{1}{3}-3ce^{-3x}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{1}{9} & =ce^{-3x}+\frac{x}{3}-y\\
\frac{1}{3} & =3ce^{-3x}+x-3y
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
y' & =3ce^{-3x}+x-3y-3ce^{-3x}\\
y' & =x-3y
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
0 & =ce^{-3\left(0\right)}+\frac{0}{3}-\frac{1}{9}\\
 & =c-\frac{1}{9}\\
c & =\frac{1}{9}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset space ~
\end_inset


\begin_inset Formula 
\begin{align*}
\frac{dy}{dx} & =x-3y\\
\frac{dy}{dx}+3y & =x
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\mu & =e^{\int3dx}\\
 & =e^{3x}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
e^{3x}\left[\frac{dy}{dx}+3y\right] & =e^{3x}\left[x\right]\\
e^{3x}\frac{dy}{dx}+3e^{3x}y & =xe^{3x}\\
\int\frac{d}{dx}\left[e^{3x}y\right]dx & =\int xe^{3x}dx\\
e^{3x}y & =\int xe^{3x}dx\\
e^{3x}y & =\frac{1}{3}xe^{3x}-\frac{1}{9}e^{3x}+c\\
y & =\frac{1}{3}x-\frac{1}{9}+\frac{c}{e^{3x}}
\end{align*}

\end_inset


\end_layout

