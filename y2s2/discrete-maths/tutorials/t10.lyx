\begin_layout Enumerate
Let 
\begin_inset Formula $A=\left\{ 1,2,3,4,5\right\} $
\end_inset

.
 Let 
\begin_inset Formula $f=\left(5,3,2\right)$
\end_inset

 and 
\begin_inset Formula $g=\left(3,4,1\right)$
\end_inset

 be permutations of 
\begin_inset Formula $A$
\end_inset

.
 Compute each of the following and write the results as the product of disjoint
 cycles.
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $f\circ g$
\end_inset


\begin_inset Formula 
\begin{align*}
f & =\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
1 & 5 & 2 & 4 & 3
\end{pmatrix}\\
g & =\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
3 & 2 & 4 & 1 & 5
\end{pmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
f\circ g & =\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
2 & 5 & 4 & 1 & 3
\end{pmatrix}\\
 & =\left(1,2,5,3,4\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $f^{-1}\circ g^{-1}$
\end_inset


\begin_inset Formula 
\begin{align*}
f^{-1} & =\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
1 & 3 & 5 & 4 & 2
\end{pmatrix}\\
g^{-1} & =\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
4 & 2 & 1 & 3 & 5
\end{pmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
1 & 3 & 5 & 4 & 2
\end{pmatrix}\circ\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
4 & 2 & 1 & 3 & 5
\end{pmatrix} & =\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
1 & 3 & 5 & 4 & 2
\end{pmatrix}\circ\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
4 & 2 & 1 & 3 & 5
\end{pmatrix}\\
 & =\begin{pmatrix}1 & 2 & 3 & 4 & 5\\
4 & 3 & 1 & 5 & 2
\end{pmatrix}\\
 & =\left(1,4,5,2,3\right)
\end{align*}

\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate
Let 
\begin_inset Formula $A=\left\{ 1,2,3,4,5,6\right\} $
\end_inset

 and 
\begin_inset Formula $p=\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
2 & 4 & 3 & 1 & 5 & 6
\end{pmatrix}$
\end_inset

 be a permutation of 
\begin_inset Formula $A$
\end_inset

.
\end_layout

\begin_deeper
\begin_layout Enumerate
Write 
\begin_inset Formula $p$
\end_inset

 as a product of disjoint cycles.
\begin_inset Formula 
\begin{align*}
p & =\left(1,2,4\right)\circ\left(3\right)\circ\left(5\right)\circ\left(6\right)\\
 & =\left(1,2,4\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
Compute 
\begin_inset Formula $p^{-1}$
\end_inset


\begin_inset Formula 
\begin{align*}
p^{-1} & =\begin{pmatrix}2 & 4 & 3 & 1 & 5 & 6\\
1 & 2 & 3 & 4 & 5 & 6
\end{pmatrix}\\
 & =\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
4 & 1 & 3 & 2 & 5 & 6
\end{pmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\[
p^{-1}=\left(1,4\right)\circ\left(3\right)\circ\left(5\right)\circ\left(6\right)
\]

\end_inset


\end_layout

\begin_layout Enumerate
Compute 
\begin_inset Formula $p^{2}$
\end_inset

 (basically, hop two times)
\begin_inset Formula 
\begin{align*}
p^{2} & =p\circ p\\
 & =\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
2 & 4 & 3 & 1 & 5 & 6
\end{pmatrix}\circ\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
2 & 4 & 3 & 1 & 5 & 6
\end{pmatrix}\\
 & =\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
4 & 1 & 3 & 2 & 5 & 6
\end{pmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
p^{2} & =\left(1,4,2\right)\circ\left(3\right)\circ\left(5\right)\circ\left(6\right)\\
 & =\left(1,4,2\right)
\end{align*}

\end_inset


\end_layout

\end_deeper
