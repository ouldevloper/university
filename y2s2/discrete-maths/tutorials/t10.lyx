\begin_layout Enumerate
Let 
\begin_inset Formula $A=\left\{ 1,2,3,4,5,6\right\} $
\end_inset

 and 
\begin_inset Formula $p_{1}=\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
3 & 4 & 1 & 2 & 6 & 5
\end{pmatrix}$
\end_inset

, 
\begin_inset Formula $p_{2}=\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
2 & 3 & 1 & 5 & 4 & 6
\end{pmatrix}$
\end_inset

, 
\begin_inset Formula $p_{3}=\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
6 & 3 & 2 & 5 & 4 & 1
\end{pmatrix}$
\end_inset

.
 Compute the following.
 
\end_layout

\begin_deeper
\begin_layout Enumerate
\begin_inset Formula $p_{1}^{-1}$
\end_inset


\begin_inset Formula 
\begin{align*}
p_{1}^{-1} & =\begin{pmatrix}3 & 4 & 1 & 2 & 6 & 5\\
1 & 2 & 3 & 4 & 5 & 6
\end{pmatrix}\\
 & =\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
3 & 4 & 1 & 2 & 6 & 5
\end{pmatrix}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $p_{3}\circ p_{1}$
\end_inset


\begin_inset Formula 
\begin{align*}
\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
6 & 3 & 2 & 5 & 4 & 1
\end{pmatrix}\circ\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
3 & 4 & 1 & 2 & 6 & 5
\end{pmatrix} & =\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
2 & 5 & 6 & 3 & 1 & 4
\end{pmatrix}\\
 & =\left(1,2,5\right)\circ\left(3,6,4\right)
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $p_{3}^{-1}$
\end_inset


\begin_inset Formula 
\begin{align*}
p_{3}^{-1} & =\begin{pmatrix}6 & 3 & 2 & 5 & 4 & 1\\
1 & 2 & 3 & 4 & 5 & 6
\end{pmatrix}\\
 & =\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
6 & 3 & 2 & 5 & 4 & 1
\end{pmatrix}
\end{align*}

\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $p_{1}^{-1}\circ p_{2}^{-1}$
\end_inset


\begin_inset Formula 
\begin{align*}
p_{2}^{-1} & =\begin{pmatrix}2 & 3 & 1 & 5 & 4 & 6\\
1 & 2 & 3 & 4 & 5 & 6
\end{pmatrix}\\
 & =\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
3 & 1 & 2 & 5 & 4 & 6
\end{pmatrix}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
p_{1}^{-1}\circ p_{2}^{-1} & =\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
3 & 4 & 1 & 2 & 6 & 5
\end{pmatrix}\circ\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
3 & 1 & 2 & 5 & 4 & 6
\end{pmatrix}\\
 & =\begin{pmatrix}1 & 2 & 3 & 4 & 5 & 6\\
1 & 3 & 4 & 6 & 2 & 5
\end{pmatrix}
\end{align*}

\end_inset


\end_layout

\end_deeper
